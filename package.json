{
  "name": "sparkswap-selfclaim-test",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "src/database/schema.prisma",
    "seed": "ts-node src/database/seed.ts"
  },
  "engines": {
    "node": ">=18",
    "yarn": ">=1.22.19",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "scripts": "npm run",
    "setup": "sh init.sh",
    "dev": "next dev",
    "dev:test": "npx dotenv -e .env.test next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "deploy": "next build && next export && npx thirdweb@latest upload out",
    "test": "jest --watch ./src/tests/jest",
    "####---TEMPLATE SCRIPTS---#####": "----",
    "make:template": "hygen generator new",
    "make:help": "hygen make help",
    "make:crud": "hygen make crud",
    "make:api": "hygen make api",
    "make:controller": "hygen make controller",
    "make:request": "hygen make request",
    "make:model": "hygen make model",
    "make:model:only": "hygen make model:only",
    "make:model:table": "hygen make model:table",
    "make:seeder": "hygen make model:seeder",
    "make:factory": "hygen make model:factory",
    "make:test": "hygen make test",
    "postmake:model": "npx prisma generate",
    "postmake:model:table": "npx prisma generate",
    "####---DATABASE/PRISMA SCRIPTS---#####": "----",
    "migrate:dev": "npx prisma migrate dev && dotenv -e .env.test -- prisma migrate dev",
    "migrate:reset": "npx prisma migrate reset",
    "db:seed:reset": "npx prisma db push --force-reset && npx prisma db seed",
    "db:seed": "npx prisma db seed",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "production:build": "npx prisma generate && npx prisma migrate deploy && next build"
  },
  "dependencies": {
    "@faker-js/faker": "^8.2.0",
    "@medardm/merkle-distributor": "^1.1.7",
    "@prisma/client": "^5.6.0",
    "@thirdweb-dev/chains": "^0.1.61",
    "@thirdweb-dev/react": "^4.1.14",
    "@thirdweb-dev/sdk": "^4.0.21",
    "abitype": "^0.10.3",
    "autoprefixer": "10.4.14",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "ethers": "^5.7.2",
    "formidable": "^3.5.1",
    "http-status-codes": "^2.3.0",
    "hygen": "^6.2.11",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "^13.4.13",
    "nextjs-progressbar": "^0.0.16",
    "postcss": "8.4.26",
    "react": "^18.2.0",
    "react-dom": "^18.2",
    "react-hook-form": "^7.43.0",
    "react-hot-toast": "^2.4.0",
    "superjson": "^2.2.1",
    "swr": "^2.2.4",
    "tailwindcss": "3.3.3",
    "ts-jest": "^29.1.1",
    "zod": "^3.22.4",
    "zod-validation-error": "^2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/formidable": "^3.4.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "^18",
    "eslint": "^8.29.0",
    "eslint-config-next": "^14.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.6.4",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
